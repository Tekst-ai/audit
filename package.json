  "author": "",
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "exclude": [
      "node_modules/",
      "lib/test/*.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
